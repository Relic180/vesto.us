{{? it.label}}
    <label class="slider-label" data-js="label">{{=it.label}}</label>
{{? }}

<div class="slider-track" data-js="track">
    <div class="slider-progress" data-js="progress"></div>
    <span class="slider-handle" data-js="handle">

        {{? it.valueHandle}}
            <label class="slider-value" data-js="value"></label>
        {{? }}

    </div>
</div>

{{? it.minLabel || it.minLabel === 0}}
    <label class="slider-min">{{=it.minLabel}}</label>
{{? }}

{{? it.maxLabel}}
    <label class="slider-max">{{=it.maxLabel}}</label>
{{? }}

{{? !it.valueHide && !it.valueHandle}}
    <label class="slider-value" data-js="value"></label>
{{? }}
