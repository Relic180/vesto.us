<ul class="autocomplete-results">

    {{? it.items.length}}
        {{~ it.items :item}}
        <li class="autocomplete-item" data-js="menu-option" data-val={{=item.val}}>{{=item.displayName || item.name}}</li>
        {{~ }}
    {{?? }}
        <li class="autocomplete-item" disabled>No results...</li>
    {{? }}

</ul>
