{{~ it.items: item}}

    {{? item.type === 'break'}}
        <span class="menu-break"></span>
    {{?? }}
        <a class="menu-item{{? item.class}} {{=item.class}}{{? }}" {{? item.navigate}}data-navigate="{{=item.navigate}}"{{? }} {{? item.hrefTarget}}target="{{=item.hrefTarget}}"{{? }} data-val="{{? item.val}}{{=item.val}}{{?? }}{{item.name}}{{? }}"{{? item.post}} data-post="{{=item.post}}"{{? }} data-idx="{{=item.idx}}">
            <span class="label">{{=item.name}}</span>
        </a>
    {{? }}

{{~ }}
